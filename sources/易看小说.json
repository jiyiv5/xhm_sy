{
    "name": "易看小说",
    "url": "m.yikanxiaoshuo.com",
    "version": 100,
    "search": {
        "url": "https://m.yikanxiaoshuo.com/s.php@post->type=articlename&submit=&s=${key}",
		"charset": "gbk",
        "list": "div.cover>p.line",
        "name": "a:nth-child(2)",
        "author": "a:nth-child(3)",
        "cover": "",
        "detail": "a:nth-child(2)@attr->href@replace->m.yikanxiaoshuo.com/book->www.yikanxiaoshuo.com/0"
    },
    "detail": {
        "name": "meta[property=og:novel:book_name]@attr->content",
        "author": "meta[property=og:novel:author]@attr->content",
        "cover": "meta[property=og:image]@attr->content",
        "summary": "#intro>p",
        "status": "meta[property=og:novel:status]@attr->content",
        "update": "meta[property=og:novel:update_time]@attr->content",
        "lastChapter": "div.box_con:nth-child(7)>#list>ul.chapters>li:nth-child(1)",
        "catalog": ""
    },
    "catalog": {
        "list": "div.box_con:nth-child(8)>#list>ul.chapters>li",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#content",
        "filter": ["@a"]
    }
}